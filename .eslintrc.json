{
	"env": {
		"browser": true,
		"es6": true
	},
	"extends": "eslint:recommended",
	"rules": {
		"indent": ["error", "tab", { "SwitchCase": 1 }],
		"linebreak-style": ["error", "unix"],
		"quotes": ["error", "single"],
		"semi": ["error", "always"],
		"no-console": ["warn"],
		"no-inner-declarations": ["warn"]
	},
	"globals": {
		"Promise": true,
		"$": true,
		"jQuery": true,
		"s": true,
		"TWP": true,
		"wp_pb": true,
		"wpAdTrack": true,
		"require": true,
		"module": true
	}
}
