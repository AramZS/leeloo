{
	"compilerOptions": {
		"outDir": "./build/",
		"noImplicitAny": true,
		"sourceMap": true,
		"moduleResolution": "node",
		"lib": [
			"es2016",
			"dom"
		],
		"module": "es2015",
		"target": "es5",
		"allowJs": true,
		"baseUrl": ".",
		"jsx": "react",
		"paths": {
			"*": [
				"node_modules/*",
				"src/types/*"
			]
		},
		"include": [
			"./src/**/*"
		]
	}
}
